Public Class LanguageTranslationClass
    Private mode As Integer
    Property _mode() As Integer
        Get
            Return mode
        End Get
        Set(ByVal Value As Integer)
            mode = Value
        End Set
    End Property

    Public Sub New(ByVal _mode As Integer)
        mode = _mode
    End Sub
    Public Function tranword(ByVal inword As String, Optional ByVal sendword As String = "")
        Try
            Select Case mode
                Case 0
                    Return inword
                Case 1
                    If Len(sendword) <> 0 Then
                        If inword Like "本LotはEDSリードタイムが長期になる為、投入処理後*に分割して流動してください。" Then
                            Return "Since an EDS lead time becomes a long period of time, the book Lot should divide and flow to " & _
                                    sendword & "lot after throwing-in processing."
                        ElseIf inword Like "FAB出庫票の枚数と正しいか、確認/修正して完了を押してください" & vbCrLf & "FDのWA数: *" Then
                            Return "Please check(modifies) the number of count of a FAB leaving-the-garage vote and whether it is right, and push end." & vbCrLf & _
                                   "The WA number of FDs: [  " & sendword & "  ]"
