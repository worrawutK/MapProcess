                                Return "Run"
                            Case "W:AchとBchの値の差がリミットを上回っている"
                                Return "W:The difference of the value of Ach and Bch has exceeded the limit"
                            Case "●LotNoを入力して検索"
                                Return "LotNo search"
                            Case "チャネル間差レポート"
                                Return "Channel difference report"
                            Case "オンライン上にＭＡＰフォルダが存在しません！"
                                Return "A MAP folder does not exist on network!"
                            Case "未処理異常報告書有り"
                                Return "Quality failure report exist"
                            Case "データベースへの書き込みが失敗しました！" & vbCrLf & "もう一度やり直してください。"
                                Return "The writing to the database went wrong!" & vbCrLf & "Please redo once again."
                            Case Else
                                Return inword
                        End Select
                    End If
                Case Else
                    Return inword
            End Select

        Catch ex As Exception
            Return inword
        End Try

    End Function

End Class
